             flex-direction: column;\n                            position: relative;\n\n                            span {\n                                &:nth-child(1) {\n                                    transform: none;\n                                    font-size: 10px;\n                                    margin-bottom: 4px;\n                                    transform: translateX(-26px) scale(0.8);\n                                    em {\n                                        margin-left: 2px;\n                                        font-size: 10px;\n                                    }\n                                }\n\n                                &:nth-child(2) {\n                                    transform: none;\n                                    font-size: 10px;\n                                    margin: 0;\n                                    transform: translateX(-26px) scale(0.8);\n                                }\n\n                                &:nth-child(3) {\n                                    position: absolute;\n                                    top: 50%;\n                                    right: 7px;\n                                    transform: translateY(-50%) scale(0.8);\n                                }\n                            }\n                        }\n                    }\n                }\n\n                img {\n                    flex: 0 0 30px;\n                    width: 30px;\n                    height: 30px;\n                    margin-right: 10px;\n                    transform: translateY(-14px);\n                }\n\n                .content {\n                    flex: 1;\n                    width: 100%;\n\n                    .top {\n                        display: flex;\n                        justify-content: space-between;\n                        align-items: center;\n                        padding-bottom: 8px;\n                        border-bottom: 1px solid #F3F3F3