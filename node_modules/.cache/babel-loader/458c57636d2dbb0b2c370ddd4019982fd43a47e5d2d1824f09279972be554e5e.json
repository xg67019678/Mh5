{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { getLogisticsInformation } from '@/API/order';\nimport { isLogin } from '@/utils/utis';\nexport default {\n  data() {\n    return {\n      list: []\n    };\n  },\n  mounted() {\n    const token = isLogin();\n    const {\n      id\n    } = this.$route.query;\n    getLogisticsInformation({\n      token,\n      orderId: id\n    }).then(res => {\n      this.list = res.map(item => {\n        const arr = item.log.split(id);\n        return {\n          ...item,\n          log: `${this.$t('订单')}<span class=\"log_id\">#${id}</span>${this.$t(arr[1] === '系统发货成功' ? '供应商发货成功' : arr[1])}`\n        };\n      }).reverse();\n    });\n  },\n  methods: {\n    formatTime(date) {\n      date = new Date(date);\n      let Y = date.getFullYear();\n      let M = String(date.getMonth() + 1).padStart(2, '0');\n      let D = String(date.getDate()).padStart(2, '0');\n      let h = String(date.getHours()).padStart(2, '0');\n      let m = String(date.getMinutes()).padStart(2, '0');\n      let s = String(date.getSeconds()).padStart(2, '0');\n      return `${M}/${D}/${Y} ${h}:${m}:${s}`;\n    }\n  }\n};","map":{"version":3,"names":["getLogisticsInformation","isLogin","data","list","mounted","token","id","$route","query","orderId","then","res","map","item","arr","log","split","$t","reverse","methods","formatTime","date","Date","Y","getFullYear","M","String","getMonth","padStart","D","getDate","h","getHours","m","getMinutes","s","getSeconds"],"sources":["src/page/Order/Logistics/index.vue"],"sourcesContent":["<template>\n    <div class=\"logistics\">\n        <van-nav-bar safe-area-inset-top fixed :title=\"$t('查看物流')\" left-arrow @click-left=\"() => $router.back()\" />\n        <van-steps direction=\"vertical\" :active=\"0\">\n            <van-step v-for=\"(item, index) of list\" :key=\"index\">\n                <p class=\"log\" v-html=\"item.log\"></p>\n                <p>{{ $formatZoneDate(item.updateTime) }}</p>\n            </van-step>\n        </van-steps>\n    </div>\n</template>\n\n<script>\nimport { getLogisticsInformation } from '@/API/order'\nimport { isLogin } from '@/utils/utis'\nexport default {\n    data() {\n        return {\n            list: []\n        }\n    },\n    mounted() {\n        const token = isLogin()\n        const { id } = this.$route.query\n        getLogisticsInformation({\n            token,\n            orderId: id\n        }).then(res => {\n            this.list = res.map(item => {\n                const arr = item.log.split(id)\n                return {\n                    ...item,\n                    log: `${this.$t('订单')}<span class=\"log_id\">#${id}</span>${this.$t(arr[1] === '系统发货成功' ? '供应商发货成功' : arr[1])}`\n                }\n            }).reverse()\n        })\n    },\n    methods: {\n        formatTime(date) {\n            date = new Date(date);\n            let Y = date.getFullYear();\n            let M = String(date.getMonth() + 1).padStart(2, '0');\n            let D = String(date.getDate()).padStart(2, '0');\n            let h = String(date.getHours()).padStart(2, '0');\n            let m = String(date.getMinutes()).padStart(2, '0');\n            let s = String(date.getSeconds()).padStart(2, '0');\n            return `${M}/${D}/${Y} ${h}:${m}:${s}`\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.logistics {\n    padding-top: 50px;\n\n    // font-size: 16px;\n    // min-height: 100vh;\n    // background-color: #f5f5f7;\n    :deep(.van-step--process) {\n        p {\n            \n            color: var(--main-color);\n        }\n    }\n\n    .log {\n        \n        margin-bottom: 2px;\n    }\n\n    .time {\n        \n        color: var(--main-color);\n    }\n}\n\n:deep(.log_id) {\n    color: #306ada !important;\n}</style>\n"],"mappings":";;AAaA,SAAAA,uBAAA;AACA,SAAAC,OAAA;AACA;EACAC,KAAA;IACA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,MAAAC,KAAA,GAAAJ,OAAA;IACA;MAAAK;IA